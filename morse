#!/usr/bin/perl

use warnings;

our %code = (
	'.-'   => 'a',
	'-...' => 'b',
	'-.-.' => 'c',
	'-..'  => 'd',
	'.'    => 'e',
	'..-.' => 'f',
	'--.'  => 'g',
	'....' => 'h',
	'..'   => 'i',
	'.---' => 'j',
	'-.-'  => 'k',
	'.-..' => 'l',
	'--'   => 'm',
	'-.'   => 'n',
	'---'  => 'o',
	'.--.' => 'p',
	'--.-' => 'q',
	'.-.'  => 'r',
	'...'  => 's',
	'-'    => 't',
	'..-'  => 'u',
	'...-' => 'v',
	'.--'  => 'w',
	'-..-' => 'x',
	'-.--' => 'y',
	'--..' => 'z',
);


sub decode($)
{
	$_ = $_[0];

	$ret = '';

	s|[^ \./-]||g;

	while(s|^([^/]*)/||){
		# $1 = [\.- ]*
		$word = $1;

		while($word =~ s| *([^ ]*) ||){
			# $1 = letter = [\.-]
			if(!$code{$1}){
				warn "Unknown code \"$1\"\n";
			}else{
				$ret .= $code{$1};
			}
		}

		$ret .= ' ';
	}

	return $ret;
}

$\ = "\n";
print decode $_ while <>;
